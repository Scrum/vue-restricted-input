{
  "name": "vue-restricted-input",
  "version": "0.0.4",
  "description": "Creates a pattern for restricted-input from data obtained from credit-card-type",
  "license": "MIT",
  "repository": "scrum/vue-restricted-input",
  "homepage": "https://github.com/scrum/vue-restricted-input#readme",
  "bugs": "scrum/vue-restricted-input/issues",
  "author": {
    "name": "Ivan Demidov",
    "email": "Scrum@list.ru",
    "url": "https://twitter.com/Scrum_"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "version": "conventional-changelog -i changelog.md -s -r 0 && git add changelog.md && git commit -m \"build: update changelog\"",
    "prepare": "npm run build",
    "build": "rimraf lib && rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc ava",
    "lint": "eslint {src,test}/*.js *.md --fix",
    "pretest": "clinton && npm run lint"
  },
  "files": [
    "lib/",
    "dist/"
  ],
  "keywords": [
    "card",
    "gaps",
    "pattern",
    "mask"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^8.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "*",
    "babel-eslint": "^10.0.2",
    "browser-env": "^3.2.6",
    "clinton": "^0.14.0",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.4",
    "credit-card-input-mask": "^2.0.1",
    "eslint": "^6.0.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-space": "^0.21.0",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "eslint-plugin-unicorn": "^9.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t",
      "pre-commit": "clinton && lint-staged",
      "commit-msg": "commitlint --extends=@commitlint/config-angular -e"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint",
    "*.md": "eslint"
  },
  "ava": {
    "files": [
      "test/test-*.js"
    ],
    "require": [
      "./test/helpers/setup.js"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "prettier",
      "html",
      "unicorn",
      "babel",
      "markdown"
    ],
    "extends": [
      "plugin:vue/recommended",
      "plugin:ava/recommended",
      "plugin:unicorn/recommended",
      "xo",
      "xo-space"
    ],
    "rules": {
      "no-new": "off",
      "capitalized-comments": "off"
    }
  },
  "clinton": {
    "ignores": [
      "test/**",
      "tmp/**",
      "lib/**",
      "*.{html,jpg}"
    ],
    "rules": {
      "pkg-main": "off",
      "xo": "off"
    }
  }
}
